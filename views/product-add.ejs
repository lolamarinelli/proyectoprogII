	<!-- Header -->
	<%- include('partials/headerLogueado.ejs') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<form action="/product" method="GET">
					<p>
						<label for="">Modelo</label>
						<input type="text" name="modelo" id="modelo">
					</p>
					<p>
						<label for="">Imagen</label>
						<input type="image" src="https://via.placeholder.com/150" alt="submit" width="150" height="150">
					</p>
					<p>
						<label for="">Descripción</label>
						<textarea name="Descripción" id="descripcion" cols="30" rows="5"></textarea>
					</p>
					<p>
						<button type="submit">Agregar Producto</button>
					</p>
			</form>
			</h2>


	<!-- footer -->
	<%- include('partials/footer.ejs') %>
</body>

</html>