	<!-- Header -->
	<%- include('partials/headerLogueado.ejs') %>
	<!-- /Header -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Editar mi perfil</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<h2 class="col-12"> 
				<form action="/profile" method="GET">
					<p>
						<label for="">Editar nombre</label>
						<input type="text" name="nombre" id="nombre">
					</p>
					<p>
						<label for="">Editar apellido</label>
						<input type="text" name="apellido" id="apellido">
					</p>
					<p>
						<label for="">Editar fecha de nacimiento</label>
						<input type="date">
					</p>
					<p>
						<label for="">Editar Email</label>
						<input type="email" name="email" id="email">
					</p>
					<p>
						<label for="">Cambiar contraseña</label>
						<input type="password" name="contraseña" id="contraseña">
					</p>
				
					
					<p>
						<button type="submit">Guardar</button>
					</p>

			</form>
			</h2>


	<!-- footer -->
	<%- include('partials/footer.ejs') %>
	
</body>

</html>